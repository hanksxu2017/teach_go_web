<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.jeeweb.modules.sys.mapper.StudentMapper" >
  <resultMap id="BaseResultMap" type="cn.jeeweb.modules.sys.entity.Student" >
    <id column="id" property="id" jdbcType="VARCHAR" />
    <result column="real_name" property="realName" jdbcType="VARCHAR" />
    <result column="level" property="level" jdbcType="INTEGER" />
    <result column="salt" property="salt" jdbcType="VARCHAR" />
    <result column="remain_course" property="remainCourse" jdbcType="INTEGER" />
    <result column="status" property="status" jdbcType="INTEGER" />
    <result column="create_date" property="createDate" jdbcType="TIMESTAMP" />
  </resultMap>
  
  <sql id="Base_Column_List" >
    DISTINCT
    t.id, 
    t.real_name,
    t.level,
    t.salt,
    t.remain_course,
    t.status,
    t.create_date
  </sql>

  <select id="selectStudentList" resultMap="BaseResultMap">
	  select 
   	    <include refid="Base_Column_List" />
      from tg_student t
       <if test="ew.sqlSegment != '' and ew.sqlSegment != null">
		  <where>
		    ${ew.sqlSegment} 
		  </where>
	   </if> 
  </select>

</mapper>